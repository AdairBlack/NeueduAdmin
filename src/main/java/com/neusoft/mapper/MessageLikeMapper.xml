<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mapper.MessageLikeMapper">

	<select id = "findMessageLikeById" parameterType = "int" resultType = "com.neusoft.bean.MessageLike">
		select * 
		from messagelike
		where mid = #{mid}
	</select>

</mapper>